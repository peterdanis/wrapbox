{
  "compilerOptions": {
    "target": "es2018",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "isolatedModules": true,
    "jsx": "preserve",
    "outDir": "./build",
    "sourceMap": true
  },
  "include": ["src/electron"]
}
